<h2>Results index</h2>

<table class="table table-hover table-inverse">
  <% if @posts.present? %>
    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td><%= post.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% elsif @events.present? %>
    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= event.title %></td>
        </tr>
      <% end %>
    </tbody>
    <% elsif @jitikais.present? %>
    <tbody>
      <% @jitikais.each do |jitikai| %>
        <tr>
          <td><%= jitikai.title %></td>
        </tr>
      <% end %>
    </tbody>
    <% elsif @dusts.present? %>
    <tbody>
      <% @dusts.each do |dust| %>
        <tr>
          <td><%= dust.title %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>